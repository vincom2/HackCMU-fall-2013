{
  "manifest_version": 2,

  "name": "Piazza extension test",
  "version": "1.0",
  "description": "testing stuff lulz",
  "permissions": ["*://*.piazza.com/*"],

  "content_scripts": [
  {
    "matches": ["*://piazza.com/*"],
    "js": ["lib/jquery-1.10.2.js","piazza.js"],
    "run_at": "document_end"
  }
  ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}